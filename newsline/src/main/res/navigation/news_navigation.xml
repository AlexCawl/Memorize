<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/news_navigation"
    app:startDestination="@id/current_news">

    <fragment
        android:id="@+id/current_news"
        android:name="org.alexcawl.memorize.newsline.ui.current.CurrentNewsFragment"
        android:label="fragment_current_news"
        tools:layout="@layout/fragment_current_news" />

    <fragment
        android:id="@+id/saved_news"
        android:name="org.alexcawl.memorize.newsline.ui.saved.SavedNewsFragment"
        android:label="fragment_saved_news"
        tools:layout="@layout/fragment_saved_news" />

    <fragment
        android:id="@+id/search_news"
        android:name="org.alexcawl.memorize.newsline.ui.search.SearchNewsFragment"
        android:label="fragment_search_news"
